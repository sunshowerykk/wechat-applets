<!--pages/video-item-list/video-item-list.wxml-->
<view class="page">
  <view class="header">
    <image src="{{list.audio.pic}}"></image>
    <view class="name">{{list.audio.des}}</view>
  </view>
  <view class="video-list">
    <view class="total">全部歌曲（19首）</view>
    <view class="video-item active" wx:for="{{list.sectionList}}" wx:key="{{index}}">
      <view class="video-name">{{item.name}}</view>
      <view class="video-author">{{item.author}}</view>
    </view>
  </view>
</view>

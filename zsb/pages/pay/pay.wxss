/* pages/pay/pay.wxss */
page {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: #f4f4f4;
}
.title {
  font-size: 22px;
  text-align: center;
  line-height: 30px;
  font-weight: 600;
}

.container {
  padding: 25rpx 0;
  font-size: 16px;
}

button {
  background: #f09135;
  color: #fff;
  margin: 40px 25rpx 0;
}

.order {
  padding: 35rpx 25rpx;
  line-height: 25px;
  background: #fff;
  margin-bottom: 25rpx;
}

.options {
  font-size: 14px;
  padding: 0 25rpx;
  line-height: 26px;
  color: #999;
}

.pay-type {
  padding: 25rpx 25rpx;
  background: #fff;
  margin-bottom: 25rpx;
}

.qianbao {
  padding: 30rpx 25rpx;
  background: #fff;
  margin-bottom: 20rpx;

}

.quan {
  padding: 25rpx 50rpx 25rpx 25rpx;
  background: url('data:image/png;base64,') no-repeat 700rpx center #fff;
  background-size: 20px 20px;
  display: flex;
  justify-content: space-between;
}

.pay-item {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.quan-count {
  color: #f09135;
}

.radio {
  margin-right: 10rpx;
}

.pay-banner {
  margin-top: 60px;
  background: #fff;
  color: #333;
  display: flex;
  justify-content: space-between;
  line-height: 55px;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  font-size: 16px;
  box-shadow: 0 -1px 3px 0 #ebebeb;
}

.final-pay {
  padding-left: 25rpx;
}

.pay-btn {
  padding: 0 30px;
  background: #f09135;
  color: #fff;
  font-size: 20px;
}

.red {
  color: #f09135;
  font-size: 20px;
}

/* 优惠券 */
.card {
  padding: 25rpx 25px;
  border-radius: 5px;
  background: #fff;
  margin: 20rpx 0;
  display: flex;
  align-items: center;
}

.card-size {
  padding: 25px;
  color: #f09135;
  font-size: 30px;
  margin-right: 20px;
}

.card-name {
  font-size: 20px;
  color: #333;
  margin-bottom: 5rpx;
}

.card-time {
  color: #999;
  font-size: 12px;
}

.card-list {
  padding: 0 25rpx;
}

.no-quan {
  background: #fff;
  padding: 10rpx 25rpx;
  margin: 0 0 20rpx;
}
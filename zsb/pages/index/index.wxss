.header-banner {
  width: 750rpx;
  height: 300rpx;
}

image {
  vertical-align: middle;
}

.header {
  position: relative;
}

.header-ban {
  position: absolute;
  top: 10%;
  right: 3%;
  font-size: 14px;
  width: 380rpx;
  background: rgba(255, 255, 255, .56);
  padding-top: 20rpx;
  text-align: center;
}

.header-more {
    width: 50%;
    height: 54rpx;
    line-height: 54rpx;
    background: #b00015;
    border-radius: 15px;
    -moz-border-radius: 15px;
    -webkit-border-radius: 5px;
    margin: 25rpx auto 10rpx;
    display: inline-block;
    color: #fff;
}

.notify {
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
    position: relative;
    padding: 0 55rpx;
    color: #333;
    font-size: 20rpx;
    background: url("data:image/png;base64,") no-repeat 20rpx center #f4f3f3;
}

.section {
  text-align: center;
}

.section-header {
  color: #db2c1b;
  line-height: 40px;
  font-weight: 700;
  text-align: center;
  margin-top: 20px;
  font-size: 20px;
}

.section-subtitle {
  font-size: 12px;
  color: #999;
  font-weight: 400;
  text-align: center;
}

.em {
  width: 24px;
  height: 4px;
  display: inline-block;
  background: #1e68c0;
  overflow: hidden;
}

.info-list {
  display: flex;
  padding: 0 30rpx;
  justify-content: space-between;
  margin: 20rpx 0;
}

.info-item {
  padding: 30rpx 0;
  width: 200rpx;
  border: 1px solid #efefef;
  border-radius: 10rpx;
  border-top: 3px solid #efefef;
  color: #666;
}

.info-img {
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 22rpx;
}

.info-item-title {
  font-size: 14px;
}

.section2-bg {
  width: 750rpx;
  height: 281rpx;
}

.img-list-img {
  width: 200rpx;
  height: 119rpx;
}

.img-info {
  font-size: 16rpx;
  color:#999;
  margin-top: 20rpx;
}

.style-bottom-title {
  font-size: 14px;
  color: #666;
}

.style-bottom-subtitle {
  font-size: 12px;
  color: #999;
}

.style-list {
  display: flex;
  justify-content: space-between;
}
.style-item {
  width: 45%;
}

.style-img {
  width: 52px;
  height: 52px;
}

.style-top {
  width: 250rpx;
  height: 250rpx;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
  background: url("data:image/png;base64,");
  background-size: 250rpx 250rpx;
}

.style-list {
  padding: 30rpx 5%;
}
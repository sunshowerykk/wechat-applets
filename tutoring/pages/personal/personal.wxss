.page {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: #f5f5f5;
}

.header {
  overflow: hidden;
  position: relative;
  padding: 30px 0;
  display: flex;
  align-items: center;
  background: url('data:image/png;base64,')  no-repeat 680rpx center rgba(91, 131, 222, .4);
  background-size: 20px 20px;
}
.clearfix {
  content: "";
  clear: both;
}
.profile-wrapper {
  padding: 0 20px;
  vertical-align: middle;
}
image  {
  width: 80px;
  height: 80px;
  border-radius: 40px;
}

.user {
  vertical-align: middle;
  color: #fff;
}

.username {
  font-size: 24px;
  line-height: 36px;
}

.user-tip {
  font-size: 16px;
}

.operate-list {
  margin-top: 30px;
  background: #fff;
  font-size: 16px;
}
.account {
  background: url('data:image/png;base64,') 10px center no-repeat;
  background-size: 20px 22px;
}
.course {
  background: url('data:image/png;base64,') 10px center no-repeat;
  background-size: 20px 20px;
}
.order {
  background: url('data:image/png;base64,') 10px center no-repeat;
  background-size: 18px 18px;
}
.message {
  background: url('data:image/png;base64,') 10px center no-repeat;
  background-size: 18px 18px;
}

.item {
  padding: 15px 40px;
  position: relative;
  display: flex;
  justify-content: space-between;
}

.item::after {
  position: absolute;
  content: "";
  display: inline-block;
  width: 18px;
  height: 18px;
  right: 15px;
  background: url('data:image/png;base64,')  no-repeat center center ;
  background-size: 18px 18px;
}

.line::before {
  position: absolute;
  left: 40px;
  bottom: 0;
  right: 0;
  content: "";
  height: 1rpx;
  background: #e4e4e4;
}


.logout {
  margin-top: 50px;
  background: #fff;
  padding: 15px;
  text-align: center;
  color: brown;
  font-size: 16px;
}

.money {
  color: #f09135;
  font-size: 14px;
  display: flex;
  align-items: center;
}